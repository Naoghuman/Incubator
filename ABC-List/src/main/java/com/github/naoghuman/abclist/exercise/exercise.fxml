<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.VBox?>

<VBox id="VBox" prefHeight="251.0" prefWidth="389.0" spacing="14.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.naoghuman.abclist.exercise.ExercisePresenter">
    <children>
        <ToolBar minHeight="-Infinity" prefHeight="40.0">
            <items>
            <ComboBox fx:id="cbTime" prefWidth="150.0" />
                <Button fx:id="bStartExercise" mnemonicParsing="false" onAction="#onActionStartExercise" text="Start" />
            </items>
        </ToolBar>
        <ScrollPane fitToHeight="true" fitToWidth="true" VBox.vgrow="ALWAYS">
            <content>
                <VBox fx:id="vbSigns" spacing="7.0" style="-fx-background-color: GOLDENROD;" />
            </content>
        </ScrollPane>
    </children>
</VBox>
