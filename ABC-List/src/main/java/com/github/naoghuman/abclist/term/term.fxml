<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.VBox?>


<VBox id="VBox" prefHeight="400.0" prefWidth="428.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.60" fx:controller="com.github.naoghuman.abclist.term.TermPresenter">
   <children>
      <ToolBar maxWidth="1.7976931348623157E308" prefHeight="40.0">
        <items>
          <Button fx:id="bSave" disable="true" mnemonicParsing="false" onAction="#onActionSaveTerm" text="Save" />
        </items>
      </ToolBar>
      <Label text="Title">
         <VBox.margin>
            <Insets top="14.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="tfTitle" maxWidth="1.7976931348623157E308" />
      <Label text="Description">
         <VBox.margin>
            <Insets top="14.0" />
         </VBox.margin>
      </Label>
      <TextArea fx:id="taDescription" maxWidth="1.7976931348623157E308" prefHeight="128.0" wrapText="true" />
   </children>
</VBox>
