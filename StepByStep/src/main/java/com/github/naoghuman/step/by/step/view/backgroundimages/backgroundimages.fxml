<StackPane id="StackPane" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.naoghuman.step.by.step.view.backgroundimages.BackgroundImagesPresenter">
    <children>
        <ImageView fx:id="ivBackgroundImage" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
      <AnchorPane fx:id="apTileImage" prefHeight="200.0" prefWidth="200.0" />
        <ImageView fx:id="ivClippedBackgroundImage" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
        <Circle fx:id="cClippedBackgroundImage" fill="TRANSPARENT" radius="300.0" stroke="#6495ED" strokeWidth="2.0" />
    </children>
</StackPane>
